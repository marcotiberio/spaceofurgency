<div class="flyntComponent {{ options.theme }}" id="{{ preContentHtml|lower|replace({' ': ''})}}" is="flynt-block-wysiwyg-post">
  <div class="container notFullWidth">
    <div class="preContent">{{ preContentHtml|e('wp_kses_post') }}</div>
    <div class="content">
      <div class="content-left">{{ contentleftHtml|e('wp_kses_post') }}</div>
      <div class="content-right">{{ contentrightHtml|e('wp_kses_post') }}</div>
    </div>
  </div>
</div>
