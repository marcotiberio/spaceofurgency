<img src="/wp-content/uploads/2022/06/before.svg">
<div class="flyntComponent {{ options.theme }}" id="{{ preContentHtml|lower|replace({' ': ''})}}" is="flynt-accordion-default">
  {# <div class="before" style="background-image: url(/wp-content/themes/spaceofurgency/dist/assets/images/before-{{ splatterColor }}.svg)"></div> #}
  <div class="container">
    <div class="preContent notFullWidth">
      {{ preContentHtml|e('wp_kses_post')}}
    </div>
    <div class="content">
      <ul class="accordion">
        {% for panel in accordionPanels %}
          <li class="panel">
            <button class="panel-trigger h2" type="button" aria-expanded="false" aria-controls="content-{{ loop.index }}">
              {{ panel.panelTitle|e }}
            </button>
            <div class="panel-content" aria-hidden="true" id="content-{{ loop.index }}">
              <div class="panel-inner">
                {{ panel.panelContent|e('wp_kses_post') }}
              </div>
            </div>
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
