<div class="flyntComponent componentSpacing {{ options.theme }}" id="{{ preContentHtml|lower|replace({' ': ''})}}" is="flynt-list-logos">
  <div class="container">
      <div class="preContent notFullWidth">
        <h3>{{ preContentHtml|e('wp_kses_post') }}</h3>
      </div>
        {% if pagelink %}
        <a href="{{ pagelink.url }}" {{ pagelink.target ? "target='#{item.link.target}' rel='noopener noreferrer'" }} class="page-link">
          {{ pagelink.title ?: pagelink.url }}
        </a>
        {% endif %}
    <div class="content notFullWidth">
      <ul class="list">
        {% for item in items %}
          <li class="item">
            <div class="media {{- item.image.svg ? ' media--svg' }}">
              {% if item.link %}
                <a class="logo" href="{{ item.link.url|e('esc_url') }}" title="{{ item.link.title|e }}" {{ item.link.target == '_blank' ? 'target="_blank" rel="nofollow noreferrer noopener"' : 'rel="nofollow"' }}>
                  {% include 'Partials/_image.twig' with {image: item.image, link: item.link, type: options.card} only %}
                </a>
              {% else %}
                <div class="logo">
                  {% include 'Partials/_image.twig' with {image: item.image, link: item.link, type: options.card} only %}
                </div>
              {% endif %}
            </div>
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
